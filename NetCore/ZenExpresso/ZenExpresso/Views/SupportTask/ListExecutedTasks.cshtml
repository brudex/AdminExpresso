@model System.Collections.Generic.List<ZenExpressoCore.Models.ExecutedTasks>

@{
    ViewBag.Title = "Executed Tasks";

}

<div class="row">
     
    <div class="col-lg-12">
        <section class="panel default blue_title h2">
            <div class="panel-heading">@ViewBag.Title </div>
            <div class="panel-body">
                <table class="table table-bordered">
                    <thead>
                    <tr>
                        <td>Task Name</td>
                        <td>Description</td>
                        <td>Parameters Passed</td>
                        <td>TopLevelMenu</td>
                        <td>Executed By</td>
                        <td>Response Status</td>
                        <td>Date Executed</td> 
                    </tr>
                    </thead>
                    <tbody>
                    @foreach (var task in Model)
                    {
                        <tr>
                            <td>@task.taskName</td>
                            <td>@task.taskDescription</td>
                            <td>@task.parameterValues</td>
                            <td>@task.topMenu</td>
                            <td>@task.executedBy</td>
                            <td>@task.responseStatus</td>
                            <td>@task.dateExecuted</td> 
                        </tr>
                    }
                    </tbody>
                </table>
            </div>
            <div></div>
        </section>
    </div>
</div>

